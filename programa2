let
	float a,b,c,c1,d,e,f,g,h,i,j.
in
	read a;
	a:= a*a;
	b:= a+1;
	b:= b*b;
	c1:= b-1;
	c:= c1*c1;
	d:= c-5;
	d:= d*d;
	e:= c1+2;
	e:= e*e;
	f:= c1+3;
	f:= f*f;
	g:= c1+4;
	g:= g*g;
	h:= c1+5;
	h:= h*h;
	i:= c1+6;
	i:= i*i;
	j:= c1+7;
	j:= j*j;



	write a;
	write b;
	write c;
	write d;
	write e;
	write f;
	write g;
	write h;
	write i;
	write j;

end
